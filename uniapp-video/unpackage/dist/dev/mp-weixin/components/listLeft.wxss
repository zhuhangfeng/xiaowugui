
.content{
	font-size: 16px;
}
.title{
	width: 200px;
	font-size: 12px;
	margin: 10px 0;
}
.music{
	width: 100px;
	white-space: nowrap;
	overflow: hidden;
	font-size: 12px;
}
.music2{
	width: 200px;
	-webkit-animation: identifier 4s linear 0.2s infinite;
	        animation: identifier 4s linear 0.2s infinite;
}
@-webkit-keyframes identifier {
0%{
		-webkit-transform: translate3d(80%,0,0);
		        transform: translate3d(80%,0,0);
}
100%{
		-webkit-transform: translate3d(-80%,0,0);
		        transform: translate3d(-80%,0,0);
}
}
@keyframes identifier {
0%{
		-webkit-transform: translate3d(80%,0,0);
		        transform: translate3d(80%,0,0);
}
100%{
		-webkit-transform: translate3d(-80%,0,0);
		        transform: translate3d(-80%,0,0);
}
}

