
.tab{
	position: fixed;
	bottom: 0;
	left: 0;
	display: flex;
	width: 100%;
	height: 50px;
	justify-content: space-around;
	align-items: center;
	background-color: black;
	color: #fff;
}
.iconStyle{
	background-color: #fff;
	width: 30px;
	display: flex;
	justify-content: center;
	align-items: center;
	height: 25px;
	border-radius: 5px;
	color: black;
}

